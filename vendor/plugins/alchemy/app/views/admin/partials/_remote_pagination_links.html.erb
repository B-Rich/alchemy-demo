<%- if items.respond_to?(:total_pages) -%>
	<%= will_paginate(
		items,
		:previous_label => '&laquo; ' + _('pagination.previous_page'),
		:next_label => _('pagination.next_page') + ' &raquo;',
		:renderer => 'Alchemy::RemotePaginationLinkRenderer',
		:remote => {
			:with => with
		}
	) %>
<%- end -%>
