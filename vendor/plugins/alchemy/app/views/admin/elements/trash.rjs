page.remove("element_#{@element.id}")
Alchemy::Notice.show(page, _("Element trashed"))
page << "jQuery('#element_area').sortable('refresh')"
page << "Alchemy.refreshTrashWindow(#{@page_id})"
page << "jQuery('#element_trash_button .icon').addClass('full')"
page << "Alchemy.PreviewWindow.refresh()"
@element.rtf_contents.each do |content|
  page << "tinymce.get('contents_content_#{content.id}_body').remove()"
end
