<div class="bild" id="<%= element_dom_id(element) %>" <%= element_preview_code(element) -%>>
    <div class="bild_images">
        <%- element.contents.find_all_by_essence_type("EssencePicture").each do |content| -%>
        <div class="bild_image <%= content.essence.css_class %>">
            <%= render_essence_view(content, :image_size => "160x120") %>
        </div>
        <%- end -%>
    </div>
</div>
